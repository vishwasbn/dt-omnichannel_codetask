    <rules>
        <fullName>After Hours Email Case Auto-Response Rule</fullName>
        <actions>
            <name>After_Hours_email_alert</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Set_AutoResponse_Sent</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND( OR(MOD( DATEVALUE(CreatedDate) - DATE(1900, 1, 6), 7) =1, AND(MOD( DATEVALUE(CreatedDate) - DATE(1900, 1, 6), 7) =0, VALUE(MID(TEXT(CreatedDate),12,2))&lt; 10, VALUE(MID(TEXT(CreatedDate),12,2))&gt; 1), AND(MOD( DATEVALUE(CreatedDate) - DATE(1900, 1, 6), 7) &gt;= 2, VALUE(MID(TEXT(CreatedDate),12,2))&lt; 13, VALUE(MID(TEXT(CreatedDate),12,2))&gt; 1) ), NOT(CONTAINS(Subject, &apos;[Email Loop Protection]&apos;)),NOT($Permission.Bypass_Logic) )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Close Automatic Reply</fullName>
        <actions>
            <name>Close_Auto_Replies</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <booleanFilter>(1 AND 2) OR 3 OR 4 OR 5</booleanFilter>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>contains</operation>
            <value>AUTO:,[Auto-Reply],Automatic reply:</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>notEqual</operation>
            <value>Closed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.LastName</field>
            <operation>equals</operation>
            <value>MerchantCircle,Phenom,hua (LinkedIn Groups)</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.PersonEmail</field>
            <operation>contains</operation>
            <value>info@ppeonline.ca,timothysetterfield@gmail.com,postmaster@ppd.com,team@epartrade.com,pc@gtglass.com,prithivirajms@sulekha.com</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>notContain</operation>
            <value>There is a new question for your business</value>
        </criteriaItems>
        <description>Closes: Automatic reply from unavailable recipient</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Case.CreatedDate</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
