                            <tr>
                                <td>
                                </td>
                                <td>
                                    <div style="{!phoneNumber}"><a href="tel:{!relatedTo.Store_Phone_Number__c}" target="_blank"><span style="color:#3483de;text-decoration:none">{!relatedTo.Store_Phone_Number__c}</span></a></div>
                                </td>
                            </tr>
                        </table>   
                        <div style="{!defaultText};"><br/></div>


                         
                        
                        

                        <table align="left" style="border-top: 1px solid {!borderColor}; width:375px;">
