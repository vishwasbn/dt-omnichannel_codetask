                                        <a href="{!CancelUrl}"><apex:image id="imgCancels" style="{!linkImageSize}" height="35" url="{!cancel}" alt="Cancel"/></a>       
                                    </td>
                                </apex:outputPanel> 

                                
                            </tr>
                        </table>
            
                        </td>
                      </tr>
                    </table>
                    
